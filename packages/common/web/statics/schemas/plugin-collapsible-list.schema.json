{
    "$id": "https://wix-rich-content/collapsible-list-content-data-schema.json",
    "type": "object",
    "properties": {
      "config": {
        "$id": "/properties/config",
        "type": "object",
        "properties": {
          "expandState": {
            "$id": "/properties/config/properties/expandState",
            "type": "string",
            "title": "Expand State",
            "examples": ["first_expanded","expanded","collapsed"]
          },
          "expandOnlyOne": {
            "$id": "/properties/config/properties/expandOnlyOne",
            "type": "boolean",
            "title": "Expand Only One",
            "examples": [true, false]
          },
          "textWrap": {
            "$id": "/properties/config/properties/textWrap",
            "type": "string",
            "title": "Text Wrap",
            "examples": ["wrap","nowrap"]
          },
          "direction": {
            "$id": "/properties/config/properties/direction",
            "type": ["string", "boolean"],
            "title": "Direction",
            "examples": ["ltr","rtl"]
          }
        }
      },
      "pairs": {
        "$id": "/properties/pairs",
        "type": "array"
      }
    }
  }
  